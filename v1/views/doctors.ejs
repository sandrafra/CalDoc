<%- include("./partials/header")%>

<div class="container">
    <div class="container">
        <h1>Doctors</h1>
        <p>View doctors avaliable on <%=name[0]%></p>
    </div>
    <div class="row text-center">
    <% doctors.forEach(function(doctor){ %>
        <% doctor.find(TextRow =>{ %>
        <div class="col-md-3 col-sm-6">
            <div class="thumbnail">
                <h3><%= TextRow.name%></h4><h4><%= TextRow.surname%></h4>
                <div class="caption">
                    <h4>Adres</h4>
                    <h6><%= TextRow.specialization%></h6>
                    <h6><%= TextRow.email%></h6>
            </div>
        </div>

</div>
<%})%>
<%}) %>
</div>

<%- include("./partials/footer")%>