<%- include("./partials/header")%>
<div class="register">
  <image id="profileImage" src="/pictures/profilowe.png"/>
    <form action="/register" method="post">
      <input id="image" type="file" name="profile_photo" placeholder="Photo" required="" capture>        
        <label for="cars">Choose a specializations:</label>
        <select name="cars" id="cars">
          <option value="cardiolog">Cardiolog</option>
          <option value="laryngolog">Laryngolog</option>
        </select>
        <input type="text" name="name" placeholder="Name" required="required" />
        <input type="text" name="surname" placeholder="Surname" required="required" />
        <input type="email" name="email" placeholder="Email" required="required" />
        <input type="password" name="password" placeholder="Password" required="required" />
        <input type="password" name="password2" placeholder="Repeat password" required="required" />
        <button type="submit" class="btn btn-primary btn-large">Let me in.</button>
    </form>
</div>
<%- include("./partials/footer")%>