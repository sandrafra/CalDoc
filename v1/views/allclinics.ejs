<%- include("./partials/header")%>

<div class="container">
    <div class="container">
        <h1>Clinics</h1>
        <p>View the clinics avaliable on the DocCal</p>
    </div>
    <div class="row text-center">
        <% clinics.forEach(function(clinic){ %>
        <div class="col-md-3 col-sm-6">
            <div class="thumbnail">
                <h4><%= clinic.name%></h4>
                <div class="caption thumbnail">
                    <h4>Adres</h3>
                        <h6><%= clinic.city%></h6>
                        <h6><%= clinic.zipcode%></h6>
                        <h6><%= clinic.street%></h6>
                        <h6><%= clinic.email%></h6>
                        <h6><%= clinic.phone%></h6>
                </div>
                <form action="/clinics/clinic/<%=clinic.id%>/doctors" method="POST">
                    <button type="submit" class="btn btn-primary">See doctors</button>
                    </form>
            </div>
        </div>
        <%}) %>
    </div>
</div>

<%- include("./partials/footer")%>